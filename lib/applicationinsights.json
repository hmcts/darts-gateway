{
  "connectionString": "${file:/mnt/secrets/darts/app-insights-connection-string}",
  "role": {
    "name": "DARTS Gateway"
  },
  "customDimensions": {
    "environment": "local-dev-5"
  },
  "sampling": {
    "overrides": [
      {
        "telemetryType": "request",
        "attributes": [
          {
            "key": "http.url",
            "value": "https?://[^/]+/health.*",
            "matchType": "regexp"
          }
        ],
        "percentage": 1
      }
    ]
  },
  "instrumentation": {
    "redis": {
      "enabled": false
    }
  }
}
