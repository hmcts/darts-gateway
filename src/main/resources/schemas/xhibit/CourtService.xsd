	<xsd:complexType name="ActivityStructure">
		<xsd:complexContent>
			<xsd:extension base="BaseOrderRequirementStructure">
				<xsd:sequence>
					<xsd:element name="Contact" type="xsd:string" minOccurs="0"/>
					<xsd:element name="Location" type="LocationStructure" minOccurs="0"/>
					<xsd:element name="Activity" type="xsd:string" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AdvocateStructure">
		<xsd:complexContent>
			<xsd:extension base="PersonStructure">
				<xsd:sequence>
					<xsd:element name="StartDate" type="xsd:date" minOccurs="0"/>
					<xsd:element name="EndDate" type="xsd:date" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AlcoholAbstinenceAndMonitoringRequirementStructure">
		<xsd:sequence>
			<xsd:element name="Days" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AssociatedCaseStructure">
		<xsd:sequence>
			<xsd:element name="AssociatedCase" type="CaseNumberType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AttachmentTypeStructure">
		<xsd:sequence>
			<xsd:element name="Attachment" type="AttachmentTypeType" minOccurs="0" maxOccurs="3"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BailChangesStructure">
		<xsd:sequence>
			<xsd:element name="ChangeType" type="BailChangeType" minOccurs="0"/>
			<xsd:element name="AmendedVariedDate" type="xsd:date" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BaseOrderRequirementStructure">
		<xsd:sequence>
			<xsd:element name="Duration" type="OrderDurationType" minOccurs="0"/>
			<xsd:element name="AdditionalInformation" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BreachStructure">
		<xsd:sequence>
			<xsd:element name="OriginatingCourt">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="OriginatingCourtStructure">
							<xsd:sequence>
								<xsd:element name="OriginalOrderType" type="xsd:string"/>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="BreachCourt">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="OriginatingCourtStructure">
							<xsd:sequence>
								<xsd:element name="SentToCrownCourt" type="YesNoType"/>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CasesStructure">
		<xsd:sequence>
			<xsd:element name="Case" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="CaseNumber" type="CaseNumberType"/>
						<xsd:element name="CaseArrivedFrom" minOccurs="0">
							<xsd:complexType>
								<xsd:choice>
									<xsd:element name="OriginatingCourt" type="CourtHouseStructure"/>
									<xsd:element name="Section53" type="YesNoType" default="yes"/>
								</xsd:choice>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Hearing" type="HearingTypeStructure" minOccurs="0"/>
						<xsd:element name="Defendants" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Defendant" type="DefendantStructure"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Prosecution" type="ProsecutionStructure" minOccurs="0"/>
						<xsd:element name="AppealCaseDescription" type="xsd:string" minOccurs="0"/>
						<xsd:element name="MethodOfInstigation" type="MethodOfInstigationType" minOccurs="0"/>
						<xsd:element name="DateOfInstigation" type="xsd:date" minOccurs="0"/>
						<xsd:element name="CaseClassNumber" type="xsd:positiveInteger" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ChargeStructure">
		<xsd:sequence>
			<xsd:element name="CRN" type="CRnumberType" minOccurs="0"/>
			<xsd:element name="CRESTchargeID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CaseNumber" type="CaseNumberType" minOccurs="0"/>
			<xsd:element name="OffenceStatement" type="xsd:string"/>
			<xsd:element name="OffenceLocation" type="apd:UKPostalAddressStructure" minOccurs="0"/>
			<xsd:element name="ArrestingPoliceForceCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CommittedOnBail" type="YesNoType" minOccurs="0"/>
			<xsd:element name="OffenceStartDateTime" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="OffenceEndDateTime" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="ArraignmentDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="ConvictionDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="OffenceParticulars" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CRESToffenceNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Plea" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Verdict" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Disposals" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Disposal" type="xsd:string" maxOccurs="unbounded"/>
						<xsd:element name="CRESTDisposalData" type="cs:CRESTDisposalDataStructure" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SentenceTerm" type="xsd:duration" minOccurs="0"/>
			<xsd:element name="TermType" type="TermTypeType" minOccurs="0"/>
			<xsd:element name="ForLife" type="YesNoType" default="yes" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="ChargeType" type="xsd:string" use="optional">
					</xsd:attribute>
		<xsd:attribute name="IndictmentNumber" type="xsd:int" use="optional"/>
		<xsd:attribute name="IndictmentCountNumber" type="xsd:int" use="required"/>
		<xsd:attribute name="CJSoffenceCode" type="xsd:string" use="optional"/>
		<xsd:attribute name="PNCoffenceCode" type="xsd:string" use="optional"/>
		<xsd:attribute name="HOoffenceCode" type="xsd:string" use="optional"/>
		<xsd:attribute name="Life" type="YesNoType" use="optional"/>
		<xsd:attribute name="BreachMultiple" type="xsd:int" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="CollectionCentreStructure">
		<xsd:sequence>
			<xsd:element name="CollectionCentreName" type="xsd:string"/>
			<xsd:element name="CollectionCentreAddress" type="apd:UKPostalAddressStructure" minOccurs="0"/>
			<xsd:element name="CollectionCentreTelephone" type="apd:TelephoneNumberType" minOccurs="0"/>
			<xsd:element name="CollectionCentreFax" type="apd:TelephoneNumberType" minOccurs="0"/>
			<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CreditForBailType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:positiveInteger">
				<xsd:attribute name="Section" type="xsd:string" use="optional"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CRESTDisposalDataStructure">
		<xsd:sequence>
			<xsd:element name="DisposalCode">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:string">
							<xsd:attribute name="TemplateVersion" type="xsd:integer" use="required"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="DisposalLine" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="DilSeqNo" type="xsd:integer"/>
						<xsd:element name="Data" type="xsd:string"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CommunityOrderStructure">
		<xsd:sequence>
			<xsd:element name="DocumentID" type="DocumentIDstructure"/>
			<xsd:element name="OrderHeader" type="CourtOrderHeaderStructure"/>
			<xsd:element name="CommittingCourt" type="OriginatingCourtStructure" minOccurs="0"/>
			<xsd:element name="Breach" type="BreachStructure" minOccurs="0"/>
			<xsd:element name="OriginalOrderRevoked" minOccurs="0">
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="BreachCourt" type="YesNoType" default="yes"/>
						<xsd:element name="CurrentCourt" type="YesNoType" default="yes"/>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="FailedToComply" type="YesNoType" minOccurs="0"/>
			<xsd:element name="AfterHearingFrom" type="YesNoType" minOccurs="0"/>
			<xsd:element name="PettySessionalArea" type="CourtHouseStructure" minOccurs="0"/>
			<xsd:element name="CommunityPunishment" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Hours" minOccurs="0">
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:int">
										<xsd:attribute name="TermType" type="TermTypeType" use="optional"/>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Period" type="xsd:duration" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="RehabilitationPeriod" type="xsd:duration" minOccurs="0"/>
			<xsd:element name="Conditions" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Condition" type="ConditionStructure" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="AssociatedCases" type="AssociatedCaseStructure" minOccurs="0"/>
			<xsd:element name="AdditionalNotes" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ConditionStructure">
		<xsd:sequence>
			<xsd:element name="Description" type="xsd:string"/>
			<xsd:element name="Period" type="xsd:duration" minOccurs="0"/>
			<xsd:element name="Date" type="xsd:date" minOccurs="0"/>
			<xsd:element name="Surety" type="MoneyType" minOccurs="0"/>
			<xsd:element name="Security" type="MoneyType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CourtAppearanceStructure">
		<xsd:sequence>
			<xsd:element name="AppearanceCourt" type="CourtHouseStructure"/>
			<xsd:element name="AppearanceDateTime" type="xsd:dateTime" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CourtHouseStructure">
		<xsd:sequence>
			<xsd:element name="CourtHouseType" type="CourtType"/>
			<xsd:element name="CourtHouseCode">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CourtHouseCodeType">
							<xsd:attribute name="CourtHouseShortName" use="optional">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[A-Z]{3,5}"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CourtHouseName" type="xsd:string"/>
			<xsd:element name="CourtHouseAddress" type="apd:UKPostalAddressStructure" minOccurs="0"/>
