				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="Period" type="xsd:duration"/>
						<xsd:element name="Max116" type="YesNoType" default="yes"/>
					</xsd:choice>
					<xsd:attribute name="DetentionType" type="DetentionTypeType"/>
					<xsd:attribute name="TermType" type="TermTypeType" use="optional"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TotalPeriodOfReturn" minOccurs="0">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:duration">
