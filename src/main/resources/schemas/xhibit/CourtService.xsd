	<xsd:simpleType name="AppealTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Conviction"/>
			<xsd:enumeration value="Sentenced"/>
			<xsd:enumeration value="Conviction and Sentence"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ASnumberType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="20"/>
			<xsd:maxLength value="20"/>
			<xsd:pattern value="[0-9]{2}[A-Za-z0-9]{4}[0-9]{2}[0-9]{11}[A-Za-z0-9]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AssociatedOrderType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="communityOrder"/>
			<xsd:enumeration value="suspendedSentenceOrder"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AttachmentTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="previousConvictionList"/>
			<xsd:enumeration value="preSentenceReport"/>
			<xsd:enumeration value="medicalReport"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BailChangeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Varied"/>
			<xsd:enumeration value="Amended"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BailConditionsType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Unconditional"/>
			<xsd:enumeration value="SubjectToBailOrder"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BailDecisionType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Conditional"/>
			<xsd:enumeration value="Refused"/>
			<xsd:enumeration value="Unconditional"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BailOrderedType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SubjectToConditions"/>
			<xsd:enumeration value="AlreadyCompliedWithConditions"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BailStatusType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="On bail"/>
			<xsd:enumeration value="On remand"/>
			<xsd:enumeration value="In care"/>
			<xsd:enumeration value="In custody"/>
			<xsd:enumeration value="Not applicable"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CaseLinkRefType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z][0-9]{8}( FIX)?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CaseNumberType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z][0-9]{8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CaseType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Trial case"/>
			<xsd:enumeration value="Committal for sentence"/>
			<xsd:enumeration value="Appeal"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CommunityOrderType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="activity"/>
			<xsd:enumeration value="alcohol treatment"/>
			<xsd:enumeration value="attendance centre"/>
			<xsd:enumeration value="curfew"/>
			<xsd:enumeration value="drug rehabilitation"/>
			<xsd:enumeration value="exclusion"/>
			<xsd:enumeration value="mental health treatment"/>
			<xsd:enumeration value="programme"/>
			<xsd:enumeration value="prohibited activity"/>
			<xsd:enumeration value="residence"/>
			<xsd:enumeration value="supervision"/>
			<xsd:enumeration value="unpaid work"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CourtHouseCodeType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{3,4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CourtType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Crown Court"/>
			<xsd:enumeration value="Magistrates Court"/>
			<xsd:enumeration value="Youth Court"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CountType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="All"/>
			<xsd:enumeration value="Specific"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CounselTypeType">
				<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="CRnumberType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="23"/>
			<xsd:maxLength value="23"/>
			<xsd:pattern value="[0-9]{2}[A-Za-z0-9]{4}[0-9]{2}[0-9]{11}[A-Za-z0-9][0-9]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DateQualifierType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="before"/>
			<xsd:enumeration value="on or before"/>
			<xsd:enumeration value="on"/>
			<xsd:enumeration value="on or after"/>
			<xsd:enumeration value="after"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DetentionTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Detention"/>
			<xsd:enumeration value="Imprisonment"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HearingTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z][A-Z][A-Z]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MethodOfInstigationType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Committal"/>
			<xsd:enumeration value="Sending"/>
			<xsd:enumeration value="Execution"/>
			<xsd:enumeration value="Transfer"/>
			<xsd:enumeration value="Transfer certificated"/>
			<xsd:enumeration value="Voluntary bill"/>
			<xsd:enumeration value="Rehearing ordered"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NBSSOrderType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SuspendedSentenceOriginalTerm"/>
			<xsd:enumeration value="SuspendedSentenceSubstitute"/>
			<xsd:enumeration value="Fine"/>
			<xsd:enumeration value="CommunityRequirement"/>
			<xsd:enumeration value="SupervisionalPeriod"/>
			<xsd:enumeration value="OperationPeriod"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OrderTypeType">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="PNCnumberType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="12"/>
			<xsd:maxLength value="12"/>
			<xsd:pattern value="[0-9]{4}[0-9]{7}[A-Za-z0-9]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrisonerIDtype">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="ProsecutingAuthorityType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Crown Prosecution Service"/>
			<xsd:enumeration value="Customs and Excise"/>
			<xsd:enumeration value="Department of Trade and Industry"/>
			<xsd:enumeration value="Inland Revenue"/>
			<xsd:enumeration value="Other Prosecutor"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RecordSheetTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Appeal Record Sheet"/>
			<xsd:enumeration value="Committal Record Sheet"/>
			<xsd:enumeration value="Trial Record Sheet"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RemandTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Indicted"/>
			<xsd:enumeration value="Convicted"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RemandOrderTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LocalAuthority"/>
			<xsd:enumeration value="YouthDetention"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ResidenceStatusType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="resident"/>
			<xsd:enumeration value="non-resident"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SentenceLengthType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([0-9]|[1-9][0-9]*)-([0-9]|10|11)-([0-9]|[1-9][0-9]*)|life|section93-94"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SentenceTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Detention"/>
			<xsd:enumeration value="Imprisonment"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SubjectTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Defendant"/>
			<xsd:enumeration value="Appellant"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TermTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Before"/>
			<xsd:enumeration value="Concurrent"/>
			<xsd:enumeration value="Consecutive"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TrialLengthType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([0-9]|[1-9][0-9]*)-([0-9]|[1-9][0-9]*)\+?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="URNtype">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Za-z0-9]{4}[0-9]{7}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="WarrantAfterFailureType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="failureToComply"/>
			<xsd:enumeration value="failureToAttend"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="YesNoType">
		<xsd:restriction base="apd:YesNoType"/>
	</xsd:simpleType>
	<xsd:simpleType name="DeportationReason">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
